@using Microsoft.AspNetCore.Identity
@using aspnetcore_webapi.Models
@inject SignInManager<ApplicationUser> SignInManager
@inject UserManager<ApplicationUser> UserManager

@{
    var adm = new ApplicationUser
    {
        UserName = "admin"
    };

    var chkAdm = await UserManager.FindByNameAsync(adm.UserName);
    if (chkAdm == null)
    {
        <li class="nav-item">
            <a id="registerAdmin" href="#" class="nav-link text-dark" data-toggle="modal" data-target="#registeradminModal">RegisterAdmin</a>
        </li>
    }
    else
    {
        <li class="nav-item">
            <a id="register" asp-controller="Home" asp-action="Register" class="nav-link text-dark">Register</a>
        </li>
    }

}